pattern {	directives {	    messageSource 'nutkin2-messages'	    instructionDefinitions {            instruction 'nutkin2-left-side' round withKey {              row: p2,     yo, k3, ssk, k9, p1              row: p2, k1, yo, k3, ssk, k8, p1               row: p2, k2, yo, k3, ssk, k7, p1               row: p2, k3, yo, k3, ssk, k6, p1               row: p2, k4, yo, k3, ssk, k5, p1               row: p2, k5, yo, k3, ssk, k4, p1              row: p2, k6, yo, k3, ssk, k3, p1               row: p2, k7, yo, k3, ssk, k2, p1              row: p2, k8, yo, k3, ssk, k1, p1              row: p2, k9, yo, k3, ssk,     p1            }            instruction 'nutkin2-right-side' round withKey {              row: p1, k9, k2tog, k3, yo,     p2              row: p1, k8, k2tog, k3, yo, k1, p2              row: p1, k7, k2tog, k3, yo, k2, p2              row: p1, k6, k2tog, k3, yo, k3, p2              row: p1, k5, k2tog, k3, yo, k4, p2              row: p1, k4, k2tog, k3, yo, k5, p2              row: p1, k3, k2tog, k3, yo, k6, p2              row: p1, k2, k2tog, k3, yo, k7, p2              row: p1, k1, k2tog, k3, yo, k8, p2              row: p1,     k2tog, k3, yo, k9, p2            }                        mergedInstruction 'nutkin2' ['merge-point':'row',type:'physical','message-key':'instruction.nutkin2'] {              instructionRef 'nutkin2-left-side'              instructionRef 'nutkin2-right-side'			}            inlineInstruction 'gusset-right-side-decrease' { k1, ssk, k to end }            inlineInstruction 'gusset-left-side-decrease' { k to 3 before marker, k2tog, k1 }            inlineInstruction 'k1p1-ribbing' withKey { k1, p1 }            inlineInstruction 'k2p2-ribbing' withKey { k2, p2 }                        instruction 'garter-ribbing' round withKey {              row: inlineInstructionRef 'k2p2-ribbing'              row: k4            }	    }    	}	generalInformation 'en' {		name: 'Nutkin Two'		description: 'A sock pattern inspired by the Nutkin pattern by Beth LaPense'		dimensions		gauge 'stockinette' {			stitchGauge:  7 stitchesPerInch			   rowGauge: 12 rowsPerInch		}		techniques {			technique: 'lace'		}		author {			firstName: 'Meg'			lastName:  'Dedolph'		}	}	supplies {		yarns {			yarnType 'lornas-1' [brand:'Lorna\'s Laces',category:'Shepherd Sock',weight:'fingering'] {				ballWeight: 50 g			}			yarn 'main-color' [typeref:'lornas-1'] withKey {				totalLength: 430 yd				totalWeight: 100 g				color 'Mixed Berries' [number:38]			}		}		needles {			needleType 'size1circ' circ [brand:'Knitpicks'] {				length: 24 inches				size: US 1			}			needle 'needle1' [typeref:'size1circ'] withKey			needle 'needle2' [typeref:'size1circ'] withKey		}		accessories	}	directions {		instructionGroup 'start' {			useNeedle 'needle1'			co 68 'long-tail'			useNeedles silently 'needle1' 'needle2'			arrangeStitches 'needle1':34 'needle2':34			joinInRound		}		instructionGroup 'cuff' withKey {			instruction 'cuff-round' {			    round {			        repeat to end { inlineInstructionRef 'k1p1-ribbing' }			    }			}			repeat 'cuff-round' until measures 1 in		}		instructionGroup 'leg' withKey resetRowCount {            instruction 'leg-pattern' {              forEachRowInInstruction 'nutkin2' {                usingNeedle 'needle1' {                  p1, repeat 8 times { applyNextRow 'garter-ribbing' }, k1                }                usingNeedle 'needle2' {                  applyNextRow 'nutkin2'                }              }            }			repeat 'leg-pattern' 6 additionalTimes		}		instructionGroup 'heel-flap' withKey resetRowCount {			useNeedle 'needle1'			declareFlatKnitting with rightSide next			instruction 'heel-flap-pattern' {				row:  repeat to 2 before end { k1,sl 1 }, k2				row:  sl 1, p to end			}			repeat 'heel-flap-pattern' until measures 2.5 in		}		instructionGroup 'heel-turn' withKey resetRowCount {			section {				shortRow informSide {					sl 1, k18, ssk, k1, turn stateStitchesRemaining				}				shortRow {					sl 1, p5, p2tog, p1, turn stateStitchesRemaining				}			}			section {				informationalMessage 'heel-turn.explain-gap'				instruction 'heel-turn-repeat' {					shortRow {						sl 1, k to 1 before gap, ssk, k1, turn stateStitchesRemaining					}					shortRow {						sl 1, p to 1 before gap, p2tog, p1, turn stateStitchesRemaining					}				}				repeat 'heel-turn-repeat' 4 additionalTimes				shortRow: sl 1, k16, ssk, k1				row: sl 1, p17, p2tog, p1				inform 20 sts left			}			section {				row: sl 1, k9, designateEndOfRow			}		}        instructionGroup 'work-gusset' withKey resetRowCount {			useNeedles 'needle1' 'needle2'			declareRoundKnitting			arrangeStitches 'needle1':27 'needle2':27			round {				usingNeedle 'needle1' {					k10, pickUp 18 sts knitwise, placeMarker					applyNextRow 'nutkin2-left-side'				}				usingNeedle 'needle2' {					applyNextRow 'nutkin2-right-side'					placeMarker, pickUp 18 sts knitwise, k10				}			}            instruction 'gusset-rows' {              round {                  usingNeedle 'needle1' {                    inlineInstructionRef 'gusset-left-side-decrease'                    applyNextRow 'nutkin2-left-side'                  }                  usingNeedle 'needle2' {                    applyNextRow 'nutkin2-right-side'                    inlineInstructionRef 'gusset-right-side-decrease'                  }              }              round {                  usingNeedle 'needle1' {					k to marker                    applyNextRow 'nutkin2-left-side'                  }                  usingNeedle 'needle2' {                    applyNextRow 'nutkin2-right-side'					k to end                  }              }            }            repeat 'gusset-rows' until 68 sts remain	        round doNotAssignNumber {				usingNeedle 'needle1' {	k to marker, removeMarker, designateEndOfRow }		    }		    arrangeStitches 'needle2':34 'needle1':34		    labelNeedle 'needle2' withKey 'needle.instep'		    labelNeedle 'needle1' withKey 'needle.sole'		}		instructionGroup 'make-foot' withKey resetRowCount {			instruction 'foot-rows' {				round {					usingNeedle 'needle2' { p1, repeat 8 times { k2,p2 }, k1 }					usingNeedle 'needle1' {	k to end }				}			}			repeat 'foot-rows' until desiredLength		}		instructionGroup 'make-toe' withKey resetRowCount {			section {				instruction 'toe-rows' {					round {						usingNeedle 'needle2' { k1, ssk, k to 3 before end, k2tog, k1 }						usingNeedle 'needle1' {	k1, ssk, k to 3 before end, k2tog, k1 }					}					round: k to end				}				repeat 'toe-rows' until 20 st remain			}			section {				graftTogether 'needle1' 'needle2'			}		}	}}