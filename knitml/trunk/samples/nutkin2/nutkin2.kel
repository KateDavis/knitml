Pattern {	Directives {	    MessageSource 'nutkin2-text'	    InstructionDefinitions {            Instruction 'nutkin2-left-side' round withKey {              Row: p2,     yo, k3, ssk, k9, p1              Row: p2, k1, yo, k3, ssk, k8, p1               Row: p2, k2, yo, k3, ssk, k7, p1               Row: p2, k3, yo, k3, ssk, k6, p1               Row: p2, k4, yo, k3, ssk, k5, p1               Row: p2, k5, yo, k3, ssk, k4, p1              Row: p2, k6, yo, k3, ssk, k3, p1               Row: p2, k7, yo, k3, ssk, k2, p1              Row: p2, k8, yo, k3, ssk, k1, p1              Row: p2, k9, yo, k3, ssk,     p1            }            Instruction 'nutkin2-right-side' round withKey {              Row: p1, k9, k2tog, k3, yo,     p2              Row: p1, k8, k2tog, k3, yo, k1, p2              Row: p1, k7, k2tog, k3, yo, k2, p2              Row: p1, k6, k2tog, k3, yo, k3, p2              Row: p1, k5, k2tog, k3, yo, k4, p2              Row: p1, k4, k2tog, k3, yo, k5, p2              Row: p1, k3, k2tog, k3, yo, k6, p2              Row: p1, k2, k2tog, k3, yo, k7, p2              Row: p1, k1, k2tog, k3, yo, k8, p2              Row: p1,     k2tog, k3, yo, k9, p2            }                        MergedInstruction 'nutkin2' ['merge-point':'row',type:'physical','message-key':'instruction.nutkin2'] {              InstructionRef 'nutkin2-left-side'              InstructionRef 'nutkin2-right-side'			}            InlineInstruction 'gusset-right-side-decrease' { k1, ssk, k to end }            InlineInstruction 'gusset-left-side-decrease' { k to 3 before marker, k2tog, k1 }            InlineInstruction 'k1p1-ribbing' withKey { k1, p1 }            InlineInstruction 'k2p2-ribbing' withKey { k2, p2 }                        Instruction 'garter-ribbing' round withKey {              Row: ref 'k2p2-ribbing'              Row: k4            }	    }    	}	GeneralInformation "en" {		Name: "Nutkin Two"		Description: "A sock pattern inspired by the Nutkin pattern by Beth LaPensee"		Gauge 'stockinette' {			StitchGauge:  7 stitchesPerInch			RowGauge:     12 rowsPerInch		}		Techniques {			Technique: "lace"		}		Author {			FirstName: "Meg"			LastName:  "Dedolph"		}	}	Supplies {		Yarns {			YarnType 'lornas-1' [brand:"Lorna's Laces",category:"Shepherd Sock",weight:"fingering"] {				BallWeight: 50 g			}			Yarn 'main-color' [typeref:'lornas-1'] withKey {				TotalLength: 430 yd				TotalWeight: 100 g				Color "Mixed Berries" [number:38]			}		}		Needles {			NeedleType 'size1circ' circ [brand:"Knitpicks"] {				Length: 24 inches				Size: US 1			}			Needle 'needle1' [typeref:'size1circ'] withKey			Needle 'needle2' [typeref:'size1circ'] withKey		}		Accessories	}	Directions {		InstructionGroup 'start' {			UseNeedle 'needle1'			co 68 'long-tail'			UseNeedles silently 'needle1' 'needle2'			ArrangeStitches 'needle1':34 'needle2':34			JoinInRound		}		InstructionGroup 'cuff' withKey {			Instruction 'cuff-round' {			    Round {			        Repeat to end { ref 'k1p1-ribbing' }			    }			}			Repeat 'cuff-round' for 1 in		}		InstructionGroup 'leg' withKey resetRowCount {            Instruction 'leg-pattern' {              ForEachRowInInstruction 'nutkin2' {                UsingNeedle 'needle1' {                  p1, repeat 8 times { applyNextRow 'garter-ribbing' }, k1                }                UsingNeedle 'needle2' {                  ApplyNextRow 'nutkin2'                }              }            }			Repeat 'leg-pattern' 6 additionalTimes		}		InstructionGroup 'heel-flap' withKey resetRowCount {			UseNeedle 'needle1'			DeclareFlatKnitting with rightSide next			Instruction 'heel-flap-pattern' {				Row:  repeat to 2 before end { k1, sl 1 }, k2				Row:  sl 1, p to end			}			Repeat 'heel-flap-pattern' for 2.5 in		}		InstructionGroup 'heel-turn' withKey resetRowCount {			Section {				ShortRow informSide {					sl 1, k18, ssk, k1, turn stateStitchesRemaining				}				ShortRow {					sl 1, p5, p2tog, p1, turn stateStitchesRemaining				}			}			Section {				InformationalMessage 'heel-turn.explain-gap'				Instruction 'heel-turn-repeat' {					ShortRow {						sl 1, k to 1 before gap, ssk, k1, turn stateStitchesRemaining					}					ShortRow {						sl 1, p to 1 before gap, p2tog, p1, turn stateStitchesRemaining					}				}				Repeat 'heel-turn-repeat' 4 additionalTimes				ShortRow: sl 1, k16, ssk, k1				Row: sl 1, p17, p2tog, p1				Inform 20 sts left			}			Section {				Row: sl 1, k9, designateEndOfRow			}		}        InstructionGroup 'work-gusset' withKey resetRowCount {			UseNeedles 'needle1' 'needle2'			DeclareRoundKnitting			ArrangeStitches 'needle1':27 'needle2':27			Round {				UsingNeedle 'needle1' {					k10, pickUp 18 sts knitwise, placeMarker					applyNextRow 'nutkin2-left-side'				}				UsingNeedle 'needle2' {					applyNextRow 'nutkin2-right-side'					placeMarker, pickUp 18 sts knitwise, k10				}			}            Instruction 'gusset-rows' {              Round {                  UsingNeedle 'needle1' {                    ref 'gusset-left-side-decrease'                    applyNextRow 'nutkin2-left-side'                  }                  UsingNeedle 'needle2' {                    applyNextRow 'nutkin2-right-side'                    ref 'gusset-right-side-decrease'                  }              }              Round {                  UsingNeedle 'needle1' {					k to marker                    applyNextRow 'nutkin2-left-side'                  }                  UsingNeedle 'needle2' {                    applyNextRow 'nutkin2-right-side'					k to end                  }              }            }            Repeat 'gusset-rows' until 68 sts remain	        Round doNotAssignNumber {				UsingNeedle 'needle1' {	k to marker, removeMarker, designateEndOfRow }		    }		    ArrangeStitches 'needle2':34 'needle1':34		    LabelNeedle 'needle2' withKey 'needle.instep'		    LabelNeedle 'needle1' withKey 'needle.sole'		}		InstructionGroup 'make-foot' withKey resetRowCount {			Instruction 'foot-rows' {				Round {					UsingNeedle 'needle2' { p1, repeat 8 times { k2,p2 }, k1 }					UsingNeedle 'needle1' {	k to end }				}			}			Repeat 'foot-rows' until desiredLength		}		InstructionGroup 'make-toe' withKey resetRowCount {			Section {				Instruction 'toe-rows' {					Round {						UsingNeedle 'needle2' { k1, ssk, k to 3 before end, k2tog, k1 }						usingNeedle 'needle1' {	k1, ssk, k to 3 before end, k2tog, k1 }					}					Round: k to end				}				Repeat 'toe-rows' until 20 st remain			}			Section {				GraftTogether 'needle1' 'needle2'			}		}	}}