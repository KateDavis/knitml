	<library:information>
		<library:name>Export from KnitVisualizer - Simple Lace Diamonds
		</library:name>
		<library:description>Simple Lace Diamonds Library
		</library:description>
	</library:information>
	<library:directives>
		<library:namespace
			value="http://www.stitchmastery.com/knitml/charts/kv/SimpleLaceDiamonds" />
	</library:directives>
	<library:definitions>
		<instruction id="Chart1" label="Simple Lace Diamonds"
			shape="flat" row-count="28" starting-stitch-count="25">
			<chart-info stereotype="lace" hide-wrong-side="true" />
			<!-- FYI: I generated the following XML from the KnitML Pattern Editor. 
				It doesn't have to have the 'repeat to end's in it. -->
			<row number="1">
				<knit>12</knit>
				<increase type="yo" />
				<decrease type="ssk" />
				<repeat until="end">
					<knit />
				</repeat>
			</row>
			<row number="2" subsequent="even">
				<repeat until="end">
					<purl />
				</repeat>
			</row>
			<row number="3">
				<knit>10</knit>
				<decrease type="k2tog" />
				<increase type="yo" />
				<knit>1</knit>
